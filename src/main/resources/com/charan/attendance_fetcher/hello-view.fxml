<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.charan.attendance_fetcher.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>



  <Label style="-fx-font-size: 50px;">Attendance Fetcher</Label>


  <TableView fx:id="att_table"/>
  <Label fx:id="pcount">Present:</Label>
  <Label fx:id="acount">Absent:</Label>
  <Button text="Get Attendance" onAction="#onHelloButtonClick"/>


</VBox>
